{
    "name": "alxdorosenco/vatlayer-for-laravel",
    "description": "This package helps working with VatLayer service on the Laravel framework",
    "keywords": [
        "php",
        "laravel",
        "laravel-vat",
        "laravel-vatlayer",
        "vatlayer-for-laravel",
        "vatlayer",
        "vat",
        "instant-vat-number",
        "EU",
        "rates"
    ],
    "homepage": "https://github.com/alxdorosenco/vatlayer-for-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexei Dorosenco",
            "email": "alexforever1808@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AlxDorosenco\\VatlayerForLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlxDorosenco\\VatlayerForLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlxDorosenco\\VatlayerForLaravel\\VatlayerServiceProvider"
            ],
            "aliases": {
                "Vatlayer": "AlxDorosenco\\VatlayerForLaravel\\Facades\\Vatlayer"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
